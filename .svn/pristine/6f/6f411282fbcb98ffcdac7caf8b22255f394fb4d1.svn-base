package com.woodare.framework.jersey.authenticate;

import java.util.Date;

import com.woodare.framework.data.impl.AbstractData;

/**
 * 
 * @author lu_feng
 * 
 */
public class AccountData extends AbstractData {
    private static final long serialVersionUID = -2905879560343923100L;

    /** 注册类型 */
    private EnumAuthenticateMode mode;

    /** 用户ID */
    private String userId;

    /** 客户端唯一标识 */
    private String identifyCode;

    /** 社交类型 */
    private EnumSocialType socialType;

    /** 社交Id */
    private String socialId;

    /** 社交授权TOKEN */
    private String socialToken;

    /** 最后登录时间 */
    private Date lastLoginDate;

    /** 联系电话 */
    private String phoneNumber;

    /** 密码 */
    private String password;

    /** 密码 */
    private String newPassword;

    /** 昵称 */
    private String nickName;

    /** 用户姓名 */
    private String username;

    /** 邮箱 */
    private String email;

    /** 生日 */
    private String birthday;

    /** 头像附件ID */
    private String avatarAttachmentId;

    /** 仅修改密码 */
    private Boolean onlyPasswordField;

    /**
     * @return the onlyPasswordField
     */
    public Boolean getOnlyPasswordField() {
        return onlyPasswordField;
    }

    /**
     * @param onlyPasswordField
     *            the onlyPasswordField to set
     */
    public void setOnlyPasswordField(Boolean onlyPasswordField) {
        this.onlyPasswordField = onlyPasswordField;
    }

    /**
     * @return the newPassword
     */
    public String getNewPassword() {
        return newPassword;
    }

    /**
     * @param newPassword
     *            the newPassword to set
     */
    public void setNewPassword(String newPassword) {
        this.newPassword = newPassword;
    }

    /**
     * @return the username
     */
    public String getUsername() {
        return username;
    }

    /**
     * @param username
     *            the username to set
     */
    public void setUsername(String username) {
        this.username = username;
    }

    /**
     * @return the avatarAttachmentId
     */
    public String getAvatarAttachmentId() {
        return avatarAttachmentId;
    }

    /**
     * @param avatarAttachmentId
     *            the avatarAttachmentId to set
     */
    public void setAvatarAttachmentId(String avatarAttachmentId) {
        this.avatarAttachmentId = avatarAttachmentId;
    }

    /**
     * @return the userId
     */
    public String getUserId() {
        return userId;
    }

    /**
     * @param userId
     *            the userId to set
     */
    public void setUserId(String userId) {
        this.userId = userId;
    }

    /**
     * @return the socialId
     */
    public String getSocialId() {
        return socialId;
    }

    /**
     * @param socialId
     *            the socialId to set
     */
    public void setSocialId(String socialId) {
        this.socialId = socialId;
    }

    /**
     * @return the mode
     */
    public EnumAuthenticateMode getMode() {
        return mode;
    }

    /**
     * @param mode
     *            the mode to set
     */
    public void setMode(EnumAuthenticateMode mode) {
        this.mode = mode;
    }

    /**
     * @return the identifyCode
     */
    public String getIdentifyCode() {
        return identifyCode;
    }

    /**
     * @param identifyCode
     *            the identifyCode to set
     */
    public void setIdentifyCode(String identifyCode) {
        this.identifyCode = identifyCode;
    }

    /**
     * @return the socialType
     */
    public EnumSocialType getSocialType() {
        return socialType;
    }

    /**
     * @param socialType
     *            the socialType to set
     */
    public void setSocialType(EnumSocialType socialType) {
        this.socialType = socialType;
    }

    /**
     * @return the socialToken
     */
    public String getSocialToken() {
        return socialToken;
    }

    /**
     * @param socialToken
     *            the socialToken to set
     */
    public void setSocialToken(String socialToken) {
        this.socialToken = socialToken;
    }

    /**
     * @return the lastLoginDate
     */
    public Date getLastLoginDate() {
        return lastLoginDate;
    }

    /**
     * @param lastLoginDate
     *            the lastLoginDate to set
     */
    public void setLastLoginDate(Date lastLoginDate) {
        this.lastLoginDate = lastLoginDate;
    }

    /**
     * @return the phoneNumber
     */
    public String getPhoneNumber() {
        return phoneNumber;
    }

    /**
     * @param phoneNumber
     *            the phoneNumber to set
     */
    public void setPhoneNumber(String phoneNumber) {
        this.phoneNumber = phoneNumber;
    }

    /**
     * @return the password
     */
    public String getPassword() {
        return password;
    }

    /**
     * @param password
     *            the password to set
     */
    public void setPassword(String password) {
        this.password = password;
    }

    /**
     * @return the nickName
     */
    public String getNickName() {
        return nickName;
    }

    /**
     * @param nickName
     *            the nickName to set
     */
    public void setNickName(String nickName) {
        this.nickName = nickName;
    }

    /**
     * @return the email
     */
    public String getEmail() {
        return email;
    }

    /**
     * @param email
     *            the email to set
     */
    public void setEmail(String email) {
        this.email = email;
    }

    /**
     * @return the birthday
     */
    public String getBirthday() {
        return birthday;
    }

    /**
     * @param birthday
     *            the birthday to set
     */
    public void setBirthday(String birthday) {
        this.birthday = birthday;
    }
}
