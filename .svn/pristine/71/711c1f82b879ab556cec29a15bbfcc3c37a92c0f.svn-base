public class CustomMsgAuthenticationException extends AuthenticationException {

	/**
	 * 
	 * @param msg
	 */
	public CustomMsgAuthenticationException(String msg) {
		super(msg);
	}

	/**
	 * 
	 */
	private static final long serialVersionUID = 6983796874458465217L;

}
