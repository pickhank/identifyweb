package com.woodare.core.web.system.viewdata.systemroleuser;

import java.util.List;

import com.woodare.core.jpa.data.systemuser.SystemUserData;

/**
 * 
 * ClassName: SystemRoleUserViewData
 * 
 * @description
 * @author
 * @Date 2014/10/08
 * 
 */
public class SystemRoleUserViewData extends SystemUserData {
    private static final long serialVersionUID = 3273785332846163133L;

    private List<String> roleIds;

    private List<String> roleNames;

    private String resourceName;

    private Boolean isSuperFlag;

    /**
     * @return the isSuperFlag
     */
    public Boolean getIsSuperFlag() {
        return isSuperFlag;
    }

    /**
     * @param isSuperFlag
     *            the isSuperFlag to set
     */
    public void setIsSuperFlag(Boolean isSuperFlag) {
        this.isSuperFlag = isSuperFlag;
    }

    /**
     * @return the roleIds
     */
    public List<String> getRoleIds() {
        return roleIds;
    }

    /**
     * @param roleIds
     *            the roleIds to set
     */
    public void setRoleIds(List<String> roleIds) {
        this.roleIds = roleIds;
    }

    /**
     * @return the roleNames
     */
    public List<String> getRoleNames() {
        return roleNames;
    }

    /**
     * @param roleNames
     *            the roleNames to set
     */
    public void setRoleNames(List<String> roleNames) {
        this.roleNames = roleNames;
    }

    public String getResourceName() {
        return resourceName;
    }

    public void setResourceName(String resourceName) {
        this.resourceName = resourceName;
    }
}
