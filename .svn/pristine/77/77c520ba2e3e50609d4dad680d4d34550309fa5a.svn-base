package com.woodare.core.web.common.viewdata;

/**
 * 
 * @author lu_feng
 * 
 */
public class CodeAndName {

    private String name;

    private Object code;

    public static void main(String[] arfs) {

        //char c = 'a';
        char d = (char) 65;
        System.out.println(d);
    }

    /**
     * 
     * @param name
     * @param value
     */
    public CodeAndName(String name, Object code) {
        this.name = name;
        this.code = code;
    }

    /**
     * @return the name
     */
    public String getName() {
        return name;
    }

    /**
     * @param name
     *            the name to set
     */
    public void setName(String name) {
        this.name = name;
    }

    /**
     * @return the code
     */
    public Object getCode() {
        return code;
    }

    /**
     * @param code
     *            the code to set
     */
    public void setCode(Object code) {
        this.code = code;
    }
}
